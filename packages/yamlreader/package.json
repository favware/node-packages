{
  "name": "@favware/yamlreader",
  "version": "2.0.0",
  "description": "Minimal wrapper around js-yaml for directly reading in YAML files",
  "author": "@favware",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/favware/node-packages/tree/master/packages/yamlreader#readme",
  "scripts": {
    "test": "jest",
    "lint": "eslint src __tests__ --ext ts --fix -c ../../.eslintrc",
    "prebuild": "yarn lint && yarn test",
    "build": "tsc -b src",
    "start": "yarn build -w"
  },
  "dependencies": {
    "js-yaml": "^3.13.1"
  },
  "files": [
    "dist",
    "!dist/.tsbuildinfo"
  ],
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favware/node-packages.git",
    "directory": "packages/yamlreader"
  },
  "bugs": {
    "url": "https://github.com/favware/node-packages/issues"
  },
  "keywords": [
    "async",
    "data",
    "file",
    "fs",
    "parse",
    "read",
    "sync",
    "yaml",
    "yml",
    "favware",
    "typescript",
    "ts"
  ],
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/js-yaml": "^3.12.3"
  }
}
