{
  "name": "@favware/yamlreader",
  "version": "4.0.2",
  "description": "Minimal wrapper around js-yaml for directly reading in YAML files",
  "author": "@favware",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "homepage": "https://github.com/favware/node-packages/tree/main/packages/yamlreader#readme",
  "scripts": {
    "test": "vitest run",
    "lint": "eslint src tests --ext ts --fix -c ../../.eslintrc",
    "prebuild": "yarn lint && yarn test",
    "build": "tsup",
    "start": "yarn build -w"
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "tsup": "^8.0.1"
  },
  "peerDependencies": {
    "@types/js-yaml": "^4.0.9"
  },
  "peerDependenciesMeta": {
    "@types/js-yaml": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "!dist/*.tsbuildinfo"
  ],
  "engines": {
    "node": ">=v14.18.0",
    "npm": ">=7.24.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favware/node-packages.git",
    "directory": "packages/yamlreader"
  },
  "bugs": {
    "url": "https://github.com/favware/node-packages/issues"
  },
  "keywords": [
    "async",
    "data",
    "file",
    "fs",
    "parse",
    "read",
    "sync",
    "yaml",
    "yml",
    "favware",
    "typescript",
    "ts"
  ],
  "publishConfig": {
    "access": "public"
  }
}
